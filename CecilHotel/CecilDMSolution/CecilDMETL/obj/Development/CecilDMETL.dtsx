USE CecilDM

--Load FactReservation fact table

SELECT
DimCustomer.Customer_SK
, DimRoom.Room_SK
, DimHotel.Hotel_SK
, CID.Date_SK AS CheckInDate
, COD.Date_SK AS CheckOutDate
, CIT.Time_SK AS CheckInTime
, [COT].Time_SK AS CheckOutTime
, Cecil.dbo.RESERVATION.ResNumAdult AS Adults
, Cecil.dbo.RESERVATION.ResNumChild AS Children
, Cecil.dbo.RESERVATION.ResDayRate AS Rate
, Cecil.dbo.RESERVATION.ResFoodCharge AS Food
, Cecil.dbo.RESERVATION.ResPetCharge AS Pet
, Cecil.dbo.RESERVATION.ResLateCharge AS Late
, Cecil.dbo.RESERVATION.ResDamageCharge AS Damage
FROM Cecil.dbo.RESERVATION
	INNER JOIN DimCustomer
		ON Cecil.dbo.RESERVATION.CustomerID = DimCustomer.Customer_AK
	INNER JOIN DimRoom
		ON Cecil.dbo.RESERVATION.RoomID = DimRoom.Room_AK
	INNER JOIN DimHotel
		ON Cecil.dbo.RESERVATION.HotelID = DimHotel.Hotel_AK
	INNER JOIN DimDate AS CID
		ON CONVERT(DATE, Cecil.dbo.RESERVATION.ResCheckIn) = CID.Date
	INNER JOIN DimDate AS COD
		ON CONVERT(DATE, Cecil.dbo.RESERVATION.ResCheckOut) = COD.Date
	INNER JOIN DimTime AS CIT
		ON CONVERT(NCHAR(8), Cecil.dbo.RESERVATION.ResCheckIn, 108) = CIT.Time
	INNER JOIN DimTime AS [COT]
		ON CONVERT(NCHAR(8), Cecil.dbo.RESERVATION.ResCheckOut, 108) = [COT].Time;</property>
